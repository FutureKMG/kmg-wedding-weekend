{
  "name": "wedding",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:vercel": "vercel dev",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "theme:assets": "node scripts/prepare-theme-assets.mjs",
    "theme:welcome-assets": "node scripts/prepare-welcome-party-assets.mjs",
    "theme:home-assets": "node scripts/prepare-home-lounge-assets.mjs",
    "rsvp:import": "node scripts/import-rsvp-attending.mjs",
    "rsvp:check-conflicts": "node scripts/check-guest-name-conflicts.mjs",
    "test:mobile": "playwright test --config=playwright.mobile.config.ts",
    "check:mobile": "npm run build && start-server-and-test \"npm run preview -- --host 127.0.0.1 --port 4173\" http://127.0.0.1:4173 \"npm run test:mobile\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "font:build": "bash scripts/build-font.sh"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.95.3",
    "cookie": "^1.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "jose": "^6.1.3",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.13.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.10.13",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^28.1.0",
    "sharp": "^0.34.4",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.48.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  }
}
